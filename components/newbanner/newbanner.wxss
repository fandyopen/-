.banner--column {
    display: flex;
    display: -webkit-flex;
    flex-direction: column;
    -webkit-flex-direction: column;
}

.banner--row {
    display: flex;
    display: -webkit-flex;
    flex-direction: row;
    -webkit-flex-direction: row;
}

.banner--memberTap {
    padding: .25rem;
    min-width: 2.2rem;
    font-size: .5rem;
    color: #333;
    background-color: #ffdd00;
    border-radius: 10rem;
}

.banner--banner {
}

.banner--banner > .banner--swiper {
    height: 15rem;
}

.banner--swiper-item {
    width: 100%;
    padding-top: 65%;
    background-position: center;
    background-size: 100% auto;
    background-repeat: no-repeat;
    border-radius: 2px;
}

.banner--current {
    font-size: .69rem;
    color: #878787;
}

.banner--current .banner--big-title {
    color: #333 !important;
}

.banner--banner_info {
    margin-top: 1.25rem
}

.banner--banner_info .banner--banner_title {
    font-size: 1rem;
    font-weight: 500;
    color: #000;
    margin-bottom: .475rem;
    -webkit-transform: translateY(-.1rem);
    transform: translateY(-.1rem);
}

.banner--banner_info .banner--banner_summary {
    color: #999;
    font-size: .75rem;
}

.banner {
    height: 349rpx;
}

.banner navigator {
    font-size: 0;
    position: relative;
    height: 100%;
}

.banner .slide-image {
    width: 100%;
    height: 100%;
     opacity: 0;
  transition: opacity 1s linear 0.1s;
}
.banner .imageAn{  opacity: 1;}
.banner .slide-title {
    position: absolute;
    bottom: 0;
    left: 0;
    width: 100%;
    background: rgba(0,0,0,0.5);
    color: #fff;
    padding: 20rpx;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
    font-size: 11pt;
    display: none;
}
